<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
	  <modules runAllManagedModulesForAllRequests="false">
		  <remove name="WebDAVModule" />
	  </modules>
	  <handlers>
      <add name="aspNetCore" path="*" verb="GET,POST,PUT,DELETE,PATCH,DEBUG,HEAD,OPTIONS" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
    </handlers>
    <aspNetCore processPath=".\Crypto.exe" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="OutOfProcess" />
  </system.webServer>
</configuration>